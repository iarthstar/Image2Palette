{
    "name": "Image2Palette",
    "description": "Generates Color Palette from Image Layer",
    "version": "0.2.0",
    "author": "Arth K. Gajjar",
    "homepage": "https://github.com/iarthstar/Image2Palette",
    "identifier": "com.iarthstar.img-pal-gen",
    "$schema": "https://raw.githubusercontent.com/BohemianCoding/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
    "icon": "icon.png",
    "commands": [
        {
            "name": "Get Palette",
            "identifier": "img-pal-gen.get-palette",
            "shortcut": "ctrl shift p",
            "script": "./img-pal-gen.js"
        },
        {
            "name": "Set Auth",
            "identifier": "img-pal-gen.set-api-key",
            "shortcut": "ctrl shift k",
            "script": "./set-auth.js",
            "handlers": {
                "run": "onRun",
                "actions": {
                    "Startup.finish": "setAuthStr"
                }
            }
        }
    ],
    "menu": {
        "title": "ğŸï¸Img â ğŸ¨Pal",
        "items": [
            "img-pal-gen.get-palette",
            "img-pal-gen.set-api-key"
        ]
    }
}